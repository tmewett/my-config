#!/usr/bin/env bash

print_help() {
    echo "usage: t"
}

while true; do
    case "$1" in
    -h|--help)
        print_help
        exit 0
        ;;
    *)
        break
        ;;
    esac
    shift
done

if [[ -x /c/msys64/msys2.exe ]]; then
    /c/msys64/msys2.exe
    exit
fi

open -a "$T_TERMINAL_APP" .
