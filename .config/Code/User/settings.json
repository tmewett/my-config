{
    "diffEditor.renderSideBySide": false,
    "files.exclude": {
        "**/*.o": true
    },
    "workbench.editor.enablePreview": false,
    "files.insertFinalNewline": true,
    "files.hotExit": "off",
    "editor.acceptSuggestionOnEnter": "off",
    "extensions.ignoreRecommendations": true,
    "editor.quickSuggestionsDelay": 100,
    "git-graph.dialog.deleteBranch.forceDelete": true,
    "git-graph.dialog.createBranch.checkOut": true,
    "editor.find.autoFindInSelection": "multiline",
    "git-graph.dialog.rebase.ignoreDate": false,
    "git-graph.dialog.stashUncommittedChanges.includeUntracked": false,
    "editor.minimap.enabled": false,
    "git-graph.dialog.resetCurrentBranchToCommit.mode": "Hard",
    "workbench.startupEditor": "newUntitledFile",
    "window.restoreWindows": "none",
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.trimAutoWhitespace": false,
    "files.trimTrailingWhitespace": true,
    "github.gitAuthentication": false,
    "C_Cpp.dimInactiveRegions": false,
    "git.autoRepositoryDetection": "subFolders",
    "terminal.integrated.allowChords": false,
    "git-graph.commitDetailsView.location": "Docked to Bottom",
    "git-graph.markdown": false,
}
